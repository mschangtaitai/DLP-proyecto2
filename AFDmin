digraph finite_state_machine {
	rankdir=LR size=20
	node [shape=doublecircle]
	8
	10
	node [shape=square]
	2
	node [shape=circle]
	2 -> 2 [label=a]
	2 -> 2 [label=b]
	2 -> 2 [label=c]
	2 -> 2 [label=d]
	2 -> 8 [label="."]
	8 -> 8 [label=a]
	8 -> 8 [label=b]
	8 -> 8 [label=c]
	8 -> 10 [label=d]
	8 -> 2 [label="."]
	10 -> 8 [label=a]
	10 -> 8 [label=b]
	10 -> 8 [label=c]
	10 -> 10 [label=d]
	10 -> 2 [label="."]
	2 -> 2 [label="."]
}
