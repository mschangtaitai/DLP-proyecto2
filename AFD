digraph finite_state_machine {
	rankdir=LR size=20
	node [shape=doublecircle]
	4
	5
	6
	node [shape=square]
	1
	node [shape=circle]
	1 -> 2 [label=a]
	1 -> 3 [label=b]
	1 -> 3 [label=c]
	2 -> 4 [label=a]
	2 -> 5 [label=b]
	2 -> 3 [label=c]
	3 -> 3 [label=a]
	3 -> 3 [label=b]
	3 -> 3 [label=c]
	4 -> 3 [label=a]
	4 -> 3 [label=b]
	4 -> 6 [label=c]
	5 -> 3 [label=a]
	5 -> 3 [label=b]
	5 -> 6 [label=c]
	6 -> 3 [label=a]
	6 -> 3 [label=b]
	6 -> 6 [label=c]
}
